<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.161" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.UserDashboard">
   <children>
      <FlowPane layoutX="383.0" layoutY="63.0" prefHeight="200.0" prefWidth="201.0">
         <children>
            <Button fx:id="DepositButton" mnemonicParsing="false" onAction="#deposit" text="Deposit" />
            <Button fx:id="WithdrawButton" mnemonicParsing="false" onAction="#withdraw" text="Withdraw" />
            <Button fx:id="TransactionButton" mnemonicParsing="false" onAction="#transactions" text="Transactions" />
            <Button fx:id="ChangePinButton" mnemonicParsing="false" onAction="#changePin" text="Change Pin" />
            <Button fx:id="DeleteButton" mnemonicParsing="false" onAction="#deleteAccount" text="Delete This Account" />
            <Button fx:id="ExitButton" mnemonicParsing="false" onAction="#exit" text="Exit" />
            <Button fx:id="BackButton" mnemonicParsing="false" onAction="#goBack" text="Go Back" />
         </children>
      </FlowPane>
      <Label layoutX="67.0" layoutY="63.0" text="Account Name:" />
      <Label layoutX="67.0" layoutY="103.0" text="Current Balance:" />
      <Label fx:id="AccountLabel" layoutX="228.0" layoutY="63.0" text="account" />
      <Label fx:id="BalanceLabel" layoutX="228.0" layoutY="103.0" text="Balance" />
   </children>
</AnchorPane>
